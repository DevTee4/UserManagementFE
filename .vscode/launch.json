{
    "version": "0.2.0",
    "configurations": [{
        "name": "Launch Angular",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200",
        "sourceMaps": true,
        "webRoot": "${workspaceRoot}",
        "sourceMapPathOverrides": {
          "webpack:///./*": "${webRoot}/*"
        }
      },
    ]
 }